{
    "name": "kilicow/edukcate",
    "description": "KiliCow Edukcate provides an open sourced education management system on top of Laravel.",
    "keywords": ["laravel", "edukcate", "education", "lms", "saas"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Pounder",
            "email": "paul@kilicow.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "erusev/parsedown": "~1.0",
        "firebase/php-jwt": "~4.0|~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "ramsey/uuid": "^3.1",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "KiliCow\\Edukcate\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "minimum-stability": "dev"
}
